{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "lib/*": "./src/lib/*",
                    "res/*": "./src/res/*",
                    "Gruvee/*": "./*"
                }
            }
        ]
    ]
}
